<Window x:Class="DreadnoughtOvermind.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:p="clr-namespace:DreadnoughtOvermind"
        Title="MainWindow" Height="350" Width="525">
    
    <Grid>
        <GroupBox Header="Clients" HorizontalAlignment="Left" Name="groupBox1" Width="316" Margin="12,0,0,12">
            <Grid>
                <ListBox Name="listBox1" ItemsSource="{Binding Source={x:Static Application.Current}, Path=(Clients) }" DisplayMemberPath="Info" MouseDoubleClick="listBox1_SelectionChanged" HorizontalAlignment="Left" Width="141" />
            </Grid>
        </GroupBox>
    </Grid>
</Window>
